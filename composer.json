{
    "name": "open/user-bundle",
    "type": "symfony-bundle",
    "description": "OpenUserbundle based on Symfony2",
    "keywords": ["User management, OpenUserbundle"],
    "homepage": "https://github.com/justin-john/OpenUserBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin John Mathews",
            "email": "justinjohnmathews@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.1.*",
        "symfony/security-bundle": "2.1.*",
        "hwi/oauth-bundle": "*",
        "friendsofsymfony/user-bundle": "*",
        "symfony/twig-bundle": "*"
    },
    "require-dev": {
        "hwi/oauth-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Open\\UserBundle": "" }
    },
    "target-dir": "Open/UserBundle"
